#!/bin/sh
jbhelp (){
cat << EOF
Copyright 2012-2024               By Corey DeLasaux <cordelster@gmail.com>
         ____.             __________
        |    |____    _____\______   \ _______  ___
        |    \__  \  /     \|    |  _//  _ \  \/  /
    /\__|    |/ __ \|  C D  \    |   (  <_> >    <
    \________(____  /__|_|  /______  /\____/__/\_ \
                  \/      \/       \/            \/
    The DataCenter IT toolbox.             When it just needs to get done!
%+++++++++++++++++++++++++++++++ HELP INFO ++++++++++++++++++++++++++++++++%
Available Commands:

jb-help      - This screen
jb-info      - General OS/LOAD/IP information
jb-passwd    - Changes the password in system|ACF|CIFs
jb-useradd   - Add users to this systems|ACF|CIFs
jb-userdel   - Deletes user from all sytem entries
---
EOF
}
jbhelp
if [ -f /usr/bin/jb/jb-stat ]; then echo "jb-stat     - Show JamBox Web|ftp|tftp|CIFs status"; fi
if [ -f /usr/bin/screen ]; then echo "screen <dev> <buad> - screen terminal emmulator"; fi
if [ -f /usr/bin/ansible ]; then echo "ansible      - Ansible automation"; fi
if [ -f /usr/bin/cisco-reset ]; then echo "cisco-reset   - Ability to remotely factory reset and reconfigure"; fi
if [ -f /usr/bin/cisco-passwd ]; then echo "cisco-passwd  - Change password for many switches at one time"; fi
if [ -f /usr/bin/dev-backup ]; then echo "dev-backup   - Automated config backup of Cisco/NetApp/Broadcom switchs"; fi
if [ -f /usr/bin/netapp-day0 ]; then echo "netapp-day0   - Configure NetApp Cluster Filers from SP"; fi
if [ -f /usr/bin/na-health ]; then echo "na-health     - Check NetApp Clusters health";fi
if [ -f /usr/bin/netapp-inv ]; then echo "netapp-inv    - Capture system serial and HW inventory"; fi
if [ -f /usr/bin/meshtastic ]; then echo "meshtastic   - CLI for devices"; fi
if [ -f /usr/local/bin/mesh-metrics ]; then echo "mesh-metrics   - Meshtastic repeater metrics collector"; fi
if [ -f /usr/bin/mpp-solar ]; then echo "mpp-solar    - Solar inverter communications and configuration"; fi
if [ -f /usr/bin/grafana-cli ]; then echo "grafana-cli  - Grafana CLI interface"; fi
if [ -f /usr/sbin/zerotier-cli ]; then echo "zerotier-cli - ZeroTier remote network assistance (root priv required)"; fi
exit 0
